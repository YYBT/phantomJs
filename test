                            F.isDark(B, f) && H++
                        }
                    }
                    var G = Math.abs(100 * H / K / K - 50) / 5;
                    return C += 10 * G
                }
            }, v = {
                glog: function(c) {
                    if (1 > c) {
                        throw new Error("glog(" + c + ")")
                    }
                    return v.LOG_TABLE[c]
                },
                gexp: function(c) {
                    for (; 0 > c;) {
                        c += 255
                    }
                    for (; c >= 256;) {
                        c -= 255
                    }
                    return v.EXP_TABLE[c]
                },
                EXP_TABLE: new Array(256),
