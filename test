                                case A.MODE_8BIT_BYTE:
                                    return 16;
                                case A.MODE_KANJI:
                                    return 10;
                                default:
                                    throw new Error("mode:" + d)
                            }
                        } else {
                            if (!(41 > c)) {
                                throw new Error("type:" + c)
                            }
                            switch (d) {
                                case A.MODE_NUMBER:
                                    return 14;
                                case A.MODE_ALPHA_NUM:
                                    return 13;
                                case A.MODE_8BIT_BYTE:
                                    return 16;
                                case A.MODE_KANJI:
                                    return 12;
                                default:
                                    throw new Error("mode:" + d)
                            }
                        }
                    }
                },
                getLostPoint: function(F) {
                    for (var K = F.getModuleCount(), C = 0, B = 0; K > B; B++) {
