                        case k.PATTERN000:
                            return (c + f) % 2 == 0;
                        case k.PATTERN001:
                            return c % 2 == 0;
                        case k.PATTERN010:
                            return f % 3 == 0;
                        case k.PATTERN011:
                            return (c + f) % 3 == 0;
                        case k.PATTERN100:
                            return (Math.floor(c / 2) + Math.floor(f / 3)) % 2 == 0;
                        case k.PATTERN101:
                            return c * f % 2 + c * f % 3 == 0;
                        case k.PATTERN110:
                            return (c * f % 2 + c * f % 3) % 2 == 0;
                        case k.PATTERN111:
                            return (c * f % 3 + (c + f) % 2) % 2 == 0;
                        default:
                            throw new Error("bad maskPattern:" + d)
                    }
