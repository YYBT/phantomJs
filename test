                    return this.num.length
                },
                multiply: function(d) {
                    for (var c = new Array(this.getLength() + d.getLength() - 1), i = 0; i < this.getLength(); i++) {
                        for (var f = 0; f < d.getLength(); f++) {
                            c[i + f] ^= v.gexp(v.glog(this.get(i)) + v.glog(d.get(f)))
                        }
                    }
                    return new g(c, 0)
                },
                mod: function(d) {
                    if (this.getLength() - d.getLength() < 0) {
                        return this
                    }
                    for (var c = v.glog(this.get(0)) - v.glog(d.get(0)), i = new Array(this.getLength()), f = 0; f < this.getLength(); f++) {
                        i[f] = this.get(f)
                    }
                    for (var f = 0; f < d.getLength(); f++) {
                        i[f] ^= v.gexp(v.glog(d.get(f)) + c)
                    }
                    return new g(i, 0).mod(d)
                }
